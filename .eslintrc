{
  "env": {
    "jest/globals": true,
    "browser": true,
    "es6" : true
  },
  "extends": [
    "groupon"
  ],
  "plugins": ["jest"],
  "rules": {
    "node/no-unsupported-features/es-syntax": 0,
    "import/no-extraneous-dependencies": 0
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
